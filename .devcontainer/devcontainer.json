{
	"name": "cp-algorithm",
	"dockerFile": "Dockerfile",
	"runArgs": [
		"--cap-add=SYS_PTRACE",
		"--security-opt",
		"seccomp=unconfined"
	],
	"settings": { 
		"terminal.integrated.shell.linux": "/bin/bash"
	},
	"extensions": [
		"ms-vscode.cpptools",
		"bazelbuild.vscode-bazel",
		"hediet.vscode-drawio"
	],
	"postCreateCommand": "g++ -v",
}